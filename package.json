{
  "name": "nuxtswagger",
  "version": "1.1.1",
  "description": "Nuxt-TS-Swagger plugin generator CLI",
  "bin": {
    "nuxtswagger": "dist/index.js"
  },
  "scripts": {
    "alpha": "npm version prerelease --preid=alpha",
    "build": "tsc",
    "lint": "eslint --format friendly --ext .ts,.js .",
    "lint:fix": "eslint --format friendly --ext .ts,.js . --fix",
    "prepare": "simple-git-hooks"
  },
  "keywords": [
    "nuxt",
    "typescript",
    "swagger"
  ],
  "files": [
    "dist"
  ],
  "author": "Elevista",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.0",
    "cross-fetch": "^3.0.6",
    "lodash": "^4.17.20",
    "mkdirp": "^1.0.4",
    "yargs": "^16.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint"
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "plugins"
  ],
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "devDependencies": {
    "@nuxt/types": "^2.14.7",
    "@nuxtjs/axios": "^5.13.6",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.7",
    "@types/yargs": "^15.0.9",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-formatter-friendly": "^7.0.0",
    "simple-git-hooks": "^2.6.1",
    "standard": "^16.0.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.5",
    "vue": "^2.6.12",
    "vuex": "^3.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elevista/nuxtswagger.git"
  },
  "bugs": {
    "url": "https://github.com/Elevista/nuxtswagger/issues"
  },
  "homepage": "https://github.com/Elevista/nuxtswagger#readme"
}
